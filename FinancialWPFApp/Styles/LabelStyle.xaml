<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="StyledLabel" TargetType="{x:Type Label}">
        <Setter Property="Background" Value="{DynamicResource TertiaryBackgroundColor}"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource PrimaryBlackColor}"></Setter>
        <Setter Property="FontSize" Value="{DynamicResource textFontSize}"></Setter>
        <Setter Property="Padding" Value="0 0 0 5"></Setter>
    </Style>
</ResourceDictionary>